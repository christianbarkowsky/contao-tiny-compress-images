{
    "name":"christianbarkowsky/contao-tiny-compress-images",
    "description":"This plugin automatically optimizes your images (JPG, PNG, WebP) by integrating with the popular image compression services TinyJPG and TinyPNG.",
    "keywords":["contao", "tiny", "png", "jpg", "webp", "images", "compress", "optimize", "upload"],
    "type":"contao-bundle",
    "homepage": "https://plenta.io/contao-tinypng-tinyjpg-erweiterung",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Christian Barkowsky",
            "homepage":"https://plenta.io"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/christianbarkowsky"
        }
    ],
    "support":{
        "issues": "https://github.com/christianbarkowsky/contao-tiny-compress-images/issues",
        "source": "https://github.com/christianbarkowsky/contao-tiny-compress-images",
        "docs": "https://brkwsky.de/contao-tinypng-tinyjpg-erweiterung"
    },
    "require":{
        "ext-json": "*",
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.13 || ^5.1",
        "symfony/http-client": "^4.4 || ^5.4 || ^6.0"
    },
    "extra": {
        "contao-manager-plugin": "Plenta\\ContaoTinyCompressImages\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Plenta\\ContaoTinyCompressImages\\": "src/Plenta/ContaoTinyCompressImages"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "composer/package-versions-deprecated": true,
            "ocramius/package-versions": true
        }
    }
}
